Jenkinsfile (Declarative Pipeline)
pipeline {
	agent any
	stages {
		stage ("Build"){
			echo "Running Build Stage"
		}
		stage ("Test"){
			echo "Running Test Stage"
		}
		stage ("QA"){
			echo "Running QA Stage"
		}
		stage ("Deply"){
			echo "Running Deply Stage"
		}
		stage ("Monitor"){
			echo "Running Monitor Stage"
		}
	}
	


}